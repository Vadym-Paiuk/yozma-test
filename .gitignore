# Wordpress - ignore core, configuration, examples, uploads and logs.
# https://github.com/github/gitignore/blob/main/WordPress.gitignore

# Core
#
# Note: if you want to stage/commit WP core files
# you can delete this whole section/until Configuration.
/wp-admin/
/wp-content/*
/wp-includes/
/index.php
/license.txt
/readme.html
/wp-*.php
/xmlrpc.php

# Configuration
wp-config.php

# Log files
*.log

# htaccess
/.htaccess

# IDEA
/.idea

# Current Theme
!/wp-content/themes/
/wp-content/themes/*
!/wp-content/themes/yozma/
!/wp-content/themes/yozma/*

# Node Modules
node_modules